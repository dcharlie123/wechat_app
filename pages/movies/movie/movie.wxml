<import src="/pages/movies/star/star.wxml"/>
<template name="movie">
    <view class="movie-container" catchtap="ShowMovieDetail" data-movieid="{{movieId}}">
        <image class="movie-img" src="{{coverageUrl}}"></image>
        <text class="movie-title">{{title}}</text>
        <template is="star" data="{{average}}"/>
    </view>
</template>